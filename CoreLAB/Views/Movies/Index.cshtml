@{
    ViewData["Title"] = "Movies";
}
<h2>@ViewData["Title"]</h2>
<div class="container">
    <div class="row">
        <button type="button" class="btn btn-primary btn-sm" asp-area="" asp-controller="Movies" asp-action="New">New Move</button>
    </div>
    <div class="row">
        <table class="table">
            <thead class="thead-dark">
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Move</th>
                    <th scope="col">Published</th>
                    <th scope="col">Price</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">1</th>
                    <td>Mark</td>
                    <td>Otto</td>
                    <td>1.2</td>
                </tr>
                <tr>
                    <th scope="row">2</th>
                    <td>Jacob</td>
                    <td>Thornton</td>
                    <td>1.3</td>
                </tr>
                <tr>
                    <th scope="row">3</th>
                    <td>Larry</td>
                    <td>the Bird</td>
                    <td>1.2</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>


